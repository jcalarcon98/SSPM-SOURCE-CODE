      ],
      "exposed" : false
    },
    "timelineEvents" : {
      "type" : "expression",
      "value" : [
        "function executedBy(task){",
        "    if (\"completed\" === task.state) {",
        "        var executedByString = uiTranslate(\"Executed by\") + \" \";",
        "        executedByString += $data.getUserAndSubstituteDisplayName(task.executedBy, task.executedBySubstitute);",
        "        return executedByString;",
        "    } else {",
        "        return null;",
        "    }",
        "}",
        "",
        "function assignedTo(task){",
        "    var userInfo = task.assigned_id.firstname || task.assigned_id.lastName ? task.assigned_id.firstname + \" \" + task.assigned_id.lastname : task.assigned_id.userName;",
        "    return task.assigned_id ? \"Assigned to \" + userInfo : null;",
        "}",
        "",
        "function iconFor(task){",
        "    switch(task.state) {",
        "        case \"completed\": return \"check\";",
        "        case \"failed\": return \"remove-circle\";",
        "        case \"cancelled\": return \"ban-circle\";",
        "        case \"aborted\": return \"ban-circle\";",
        "        default: return \"unchecked\";",
        "    }",
        "}",
        "",
        "function styleFor(task){",
        "    switch(task.state) {",
        "        case \"completed\": return \"success\";",
        "        case \"failed\": return \"danger\";",
        "        case \"cancelled\": return \"warning\";",
        "        case \"aborted\": return \"warning\";",
        "        default: return \"primary\";",
        "    }",
        "}",
        "",
        "function toEvent(task){",
        "    return {",
        "      \"title\" : task.displayName,",
        "      \"lastUpdateDate\" : task.last_update_date,",
        "      \"executedBy\" :  executedBy(task),",
        "      \"assignedTo\" : assignedTo(task),",
        "      \"description\" : task.displayDescription,",
        "      \"icon\" : iconFor(task),",
        "      \"style\" : styleFor(task)",
        "    };",
        "}",
        "",
        "var archived = $data.archivedTasks.map(toEvent);",
        "var opened = $data.openedTasks.map(toEvent);",
        "",
        "return [].concat(opened,archived);"
      ],
      "exposed" : false
    },
